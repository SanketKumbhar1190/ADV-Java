<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.cdac.pojo.Users" table="assign1">
		<id column="name" name="name" type="java.lang.String"></id>
		<property column="username" name="userName" type="java.lang.String"></property>
		<property column="password" name="password" type="java.lang.String"></property>
		<property column="email" name="email" type="java.lang.String"></property>
		<property column="city" name="city" type="java.lang.String"></property>		
	</class>
</hibernate-mapping>
